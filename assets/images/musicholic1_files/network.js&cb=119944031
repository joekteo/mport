            m.toString().indexOf("%") > -1 && (m = i.h * parseFloat(m) / 100), h.fitToView && (o = Math.min(i.w, o), 
            p = Math.min(i.h, p)), q = Math.min(l, q), r = Math.min(l, r), o = Math.max(q, o), 
